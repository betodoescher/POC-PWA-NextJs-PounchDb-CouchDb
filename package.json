{
  "name": "next-typescript-pwa-starter",
  "version": "0.1.0",
  "description": "Unofficial Next.js + Typescript + PWA starter with a latest package",
  "author": "Danang Eko Alfianto <github.com/betodoescher>",
  "homepage": "https://github.com/betodoescher/next-typescript-pwa-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/betodoescher/POC-PWA-NextJs-PounchDb-CouchDb.git"
  },
  "bugs": {
    "url": "https://github.com/betodoescher/next-typescript-pwa-starter/issues"
  },
  "license": "MIT",
  "keywords": [
    "nextjs",
    "reactjs",
    "typescript",
    "eslint",
    "prettier",
    "husky",
    "lint-staged",
    "pwa"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "telemetry": "next telemetry",
    "lint": "next lint",
    "format": "prettier --ignore-path .gitignore \"**/*.+(js|ts|tsx|css|scss|json|md|html)\" --write",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "echo \"Write tests!\" && exit 0",
    "prepare": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.14.1",
    "@types/pouchdb": "^6.4.0",
    "add-cors-to-couchdb": "^0.0.6",
    "next": "^12.3.1",
    "next-pwa": "^5.6.0",
    "pouchdb": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@types/node": "18.8.4",
    "@types/react": "^18.0.21",
    "@types/react-dom": "18.0.6",
    "eslint": "8.25.0",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=14.19.2"
  }
}